.search {
  display: flex;
}

.am-search {
  flex: auto;
}

.filtrate {
  display: inline-block;
  line-height: 88rpx;
  color: #49a9ee;
  font-size: 32rpx;
  margin-right: 25rpx;
}

.filtrateBoard {
  width: 100%;
  height: 700rpx;
  position: absolute;
  left: 0px;
  top: 0px;
  background-color: #ffffff;
  box-shadow: 0 8rpx 16rpx 0 #c7c7c7;
}

.btn {
  display: inline-block;
  margin: 16rpx;
  width: 340rpx;
  position: absolute;
  bottom: 0rpx;
  height: 40px;
  line-height: 40px;
}